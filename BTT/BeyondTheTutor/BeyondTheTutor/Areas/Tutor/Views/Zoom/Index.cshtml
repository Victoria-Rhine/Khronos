@model IEnumerable<BeyondTheTutor.Models.TutoringAppt>
@{
    ViewBag.Title = "Index";
}

<body class="wou-silver-bg">
    <div class="ui container">
        <div id="meeting_status">

        </div>
    </div>
    
    <div class="ui centered padded grid">

        <div class="three wide column">
            <div class="ui centered vertical segment">
                <div class="ui center aligned top attached red segment">
                    <h2>
                        Zoom Information
                    </h2>
                </div>
                <div class="ui center aligned bottom attached segment" id="zoom_info_div">
                </div>
            </div>
        </div>
        <div class="six wide column">
            <div class="ui centered vertical segment">
                <div class="ui center aligned top attached red segment" style="border-bottom:none;">
                    <h2>
                        Today's Online Tutoring Appointments
                    </h2>
                </div>
                <div class="ui bottom attached segment">
                    <div class="ui relaxed divided list" id="meeting_div_current">

                    </div>

                    <div class="ui relaxed divided list" id="meeting_div_backlog">

                    </div>
                </div>
            </div>
        </div>
    </div>
</body>

@section scripts
{
    <script src="~/Scripts/main.js"></script>
    <script src="~/Scripts/zoomtutor.js"></script>
}